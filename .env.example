APP_NAME="Yukon Lifestyle"
APP_ENV=production
APP_KEY=
APP_DEBUG=false
APP_URL=https://tshirt.yukonlifestyle.com
APP_TIMEZONE=UTC
APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

# Asset URL (optional - for CDN)
ASSET_URL=

# Maintenance Mode
APP_MAINTENANCE_DRIVER=file
APP_MAINTENANCE_STORE=database

# Database Configuration
DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=yukon_lifestyle
DB_USERNAME=root
DB_PASSWORD=
DB_TIMEOUT=30

# Database URL (alternative to above - if set, overrides individual settings)
DB_URL=

# Database Socket (optional - for Unix socket connections)
DB_SOCKET=

# MySQL SSL Configuration (optional)
MYSQL_ATTR_SSL_CA=

# Redis Configuration (optional - for caching and sessions)
REDIS_CLIENT=phpredis
REDIS_CLUSTER=redis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=
REDIS_PORT=6379
REDIS_DB=0
REDIS_CACHE_DB=1
REDIS_URL=
REDIS_USERNAME=
REDIS_PREFIX=

# Session Configuration
SESSION_DRIVER=file
SESSION_LIFETIME=120
SESSION_CONNECTION=
SESSION_STORE=
SESSION_DOMAIN=
SESSION_SECURE_COOKIE=

# Mail Configuration
MAIL_MAILER=smtp
MAIL_HOST=smtp.mailgun.org
MAIL_PORT=587
MAIL_USERNAME=
MAIL_PASSWORD=
MAIL_ENCRYPTION=tls
MAIL_FROM_ADDRESS=noreply@yukonlifestyle.com
MAIL_FROM_NAME="Yukon Lifestyle"
MAIL_TIMEOUT=30
MAIL_EHLO_DOMAIN=localhost
MAIL_URL=
MAIL_LOG_CHANNEL=
MAIL_MARKDOWN_THEME=default

# Cloudflare Turnstile CAPTCHA Configuration
TURNSTILE_SITE_KEY=
TURNSTILE_SECRET_KEY=
TURNSTILE_THEME=light
TURNSTILE_SIZE=normal
TURNSTILE_LANGUAGE=auto
TURNSTILE_VALIDATE_IP=true

# Meta Pixel Configuration (optional)
META_PIXEL_ID=
META_ACCESS_TOKEN=
META_TEST_EVENT_CODE=

# Logging (optional)
LOG_CHANNEL=stack
LOG_LEVEL=error

# Broadcasting (optional)
BROADCAST_DRIVER=log

# Cache Configuration (optional)
CACHE_DRIVER=file

# Queue Configuration (optional)
QUEUE_CONNECTION=sync

# View Configuration (optional)
VIEW_COMPILED_PATH=
